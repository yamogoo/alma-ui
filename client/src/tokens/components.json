{
  "icon": {
    "xxs": {
      "size": "12 * {config.scaleFactor}"
    },
    "xs": {
      "size": "16 * {config.scaleFactor}"
    },
    "sm": {
      "size": "20 * {config.scaleFactor}"
    },
    "md": {
      "size": "24 * {config.scaleFactor}"
    },
    "lg": {
      "size": "32 * {config.scaleFactor}"
    },
    "xl": {
      "size": "48 * {config.scaleFactor}"
    },
    "xxl": {
      "size": "56 * {config.scaleFactor}"
    }
  },
  "divider": {
    "lg": {
      "borderWidth": "{tokens.stroke.sm}",
      "padding": "{tokens.spacing.md}"
    },
    "md": {
      "borderWidth": "{tokens.stroke.sm}",
      "padding": "{tokens.spacing.sm}"
    },
    "sm": {
      "borderWidth": "{tokens.stroke.sm}",
      "padding": "{tokens.spacing.xs}"
    },
    "xs": {
      "borderWidth": "{tokens.stroke.sm}",
      "padding": 0
    }
  },
  "input": {
    "md": {
      "value": {
        "fontStyle": "label-3"
      },
      "placeholder": {
        "fontStyle": "label-3"
      },
      "height": "44 * {config.scaleFactor}",
      "padding": ["{tokens.spacing.xs}", "{tokens.spacing.md}"],
      "borderRadius": "{tokens.spacing.xs}"
    },
    "lg": {
      "value": {
        "fontStyle": "label-3"
      },
      "placeholder": {
        "fontStyle": "label-3"
      },
      "height": "52 * {config.scaleFactor}",
      "padding": ["{tokens.spacing.xs}", "{tokens.spacing.md}"],
      "borderRadius": "{tokens.spacing.xs}"
    }
  },
  "button": {
    "default": {
      "lg": {
        "fontStyle": "label-1",
        "size": "56 * {config.scaleFactor}",
        "iconSize": "{components.icon.sm.size}",
        "iconSizeLiteral": "sm",
        "gap": "{tokens.spacing.sm}",
        "padding": [0, "{tokens.spacing.xl}"],
        "borderRadius": "{tokens.roundness.infinity}"
      },
      "md": {
        "fontStyle": "label-2",
        "size": "44 * {config.scaleFactor}",
        "iconSize": "{components.icon.xs.size}",
        "iconSizeLiteral": "xs",
        "gap": "{tokens.spacing.sm}",
        "padding": [0, "{tokens.spacing.lg}"],
        "borderRadius": "{tokens.roundness.infinity}"
      },
      "sm": {
        "fontStyle": "label-3",
        "size": "40 * {config.scaleFactor}",
        "iconSize": "{components.icon.xs.size}",
        "iconSizeLiteral": "xs",
        "gap": "{tokens.spacing.sm}",
        "padding": [0, "{tokens.spacing.md}"],
        "borderRadius": "{tokens.roundness.infinity}"
      },
      "xs": {
        "fontStyle": "label-3",
        "size": "36 * {config.scaleFactor}",
        "iconSize": "{components.icon.xxs.size}",
        "iconSizeLiteral": "xs",
        "gap": "{tokens.spacing.sm}",
        "padding": [0, "{tokens.spacing.sm}"],
        "borderRadius": "{tokens.roundness.infinity}"
      }
    },
    "rounded": {
      "lg": {
        "fontStyle": "label-1",
        "size": "56 * {config.scaleFactor}",
        "iconSize": "{components.icon.sm.size}",
        "iconSizeLiteral": "sm",
        "gap": "{tokens.spacing.sm}",
        "padding": [0, 0],
        "borderRadius": "{tokens.roundness.infinity}"
      },
      "md": {
        "fontStyle": "label-3",
        "size": "44 * {config.scaleFactor}",
        "iconSize": "{components.icon.xs.size}",
        "iconSizeLiteral": "xs",
        "gap": "{tokens.spacing.sm}",
        "padding": [0, 0],
        "borderRadius": "{tokens.roundness.infinity}"
      },
      "sm": {
        "fontStyle": "label-4",
        "size": "32 * {config.scaleFactor}",
        "iconSize": "{components.icon.xs.size}",
        "iconSizeLiteral": "xs",
        "gap": "{tokens.spacing.sm}",
        "padding": [0, "{tokens.spacing.lg}"],
        "borderRadius": "{tokens.roundness.infinity}"
      },
      "xs": {
        "fontStyle": "label-4",
        "size": "24 * {config.scaleFactor}",
        "iconSize": "{components.icon.xxs.size}",
        "iconSizeLiteral": "xxs",
        "gap": "{tokens.spacing.sm}",
        "padding": [0, "{tokens.spacing.lg}"],
        "borderRadius": "{tokens.roundness.infinity}"
      }
    }
  },
  "dropDown": {
    "lg": {
      "fontStyle": "label-2",
      "minWidth": "76 * {config.scaleFactor}",
      "height": "56 * {config.scaleFactor}",
      "gap": "{tokens.spacing.xs}",
      "paddingV": "{tokens.spacing.xs}",
      "paddingH": "{tokens.spacing.sm}",
      "borderRadius": "{tokens.roundness.xs}",
      "iconSize": "{components.icon.xxs.size}"
    },
    "md": {
      "fontStyle": "label-3",
      "minWidth": "76 * {config.scaleFactor}",
      "height": "44 * {config.scaleFactor}",
      "gap": "{tokens.spacing.xs}",
      "paddingV": "{tokens.spacing.xs}",
      "paddingH": "{tokens.spacing.sm}",
      "borderRadius": "{tokens.roundness.xs}",
      "iconSize": "{components.icon.xxs.size}"
    }
  },
  "options": {
    "lg": {
      "optionFontStyle": "label-2",
      "optionMinHeight": "24 * {config.scaleFactor}"
    },
    "md": {
      "optionFontStyle": "label-3",
      "optionMinHeight": "24 * {config.scaleFactor}"
    }
  },
  "group": {
    "lg": {
      "gap": "{tokens.spacing.lg}",
      "divider": "{components.divider.md.borderWidth}"
    },
    "md": {
      "gap": "{tokens.spacing.md}",
      "divider": "{components.divider.md.borderWidth}"
    },
    "sm": {
      "gap": "{tokens.spacing.sm}",
      "divider": "{components.divider.md.borderWidth}"
    }
  },
  "form": {
    "md": {
      "paddingV": "{tokens.spacing.md}",
      "paddingH": "{tokens.spacing.lg}",
      "borderRadius": "{tokens.roundness.md}"
    }
  },
  "formWrapper": {
    "md": {
      "paddingV": "{tokens.spacing.md}",
      "paddingH": "{tokens.spacing.lg}",
      "borderRadius": "{tokens.roundness.md}",
      "borderWidth": "{tokens.stroke.sm}",
      "headerPaddingV": "{tokens.spacing.sm}",
      "headerPaddingH": 0
    }
  },
  "progressbar": {
    "lg": {
      "height": "10 * {config.scaleFactor}",
      "borderRadius": "5 * {config.scaleFactor}"
    },
    "md": {
      "height": "8 * {config.scaleFactor}",
      "borderRadius": "4 * {config.scaleFactor}"
    }
  },
  "sheet": {
    "lg": {
      "gap": "{tokens.spacing.xl}",
      "buttonOffset": "{tokens.spacing.lg}",
      "padding": [
        "{tokens.spacing.xxl}",
        "{tokens.spacing.lg}",
        "{tokens.spacing.lg}",
        "{tokens.spacing.lg}"
      ]
    },
    "md": {
      "gap": "{tokens.spacing.md}",
      "buttonOffset": "{tokens.spacing.lg}",
      "padding": ["{tokens.spacing.lg}", "{tokens.spacing.lg}"]
    }
  },
  "scrollbar": {
    "width": 3,
    "borderRadius": 1
  },
  "mainContainer": {
    "respond": {
      "mobile": {
        "padding": "{tokens.spacing.xs}"
      },
      "desktop": {
        "padding": "{tokens.spacing.xs}"
      }
    },
    "roundness": "{tokens.roundness.md}"
  },
  "carouselView": {
    "lg": {
      "header-padding": ["{tokens.spacing.lg}", "{tokens.spacing.lg}"]
    },
    "md": {
      "header-padding": ["{tokens.spacing.md}", "{tokens.spacing.md}"]
    }
  },
  "pagination": {
    "lg": {
      "size": "16 * {config.scaleFactor}",
      "widthActive": "24 * {config.scaleFactor}",
      "gap": "{tokens.spacing.md}",
      "padding": "{tokens.spacing.xs}",
      "borderRadius": "8 * {config.scaleFactor}"
    },
    "md": {
      "size": "10 * {config.scaleFactor}",
      "widthActive": "24 * {config.scaleFactor}",
      "gap": "{tokens.spacing.sm}",
      "padding": "{tokens.spacing.xs}",
      "borderRadius": "5 * {config.scaleFactor}"
    }
  },
  "stepPaginationTabs": {
    "lg": {
      "fontStyle": "title-3",
      "padding": "{tokens.spacing.xs}"
    },
    "md": {
      "fontStyle": "title-4",
      "padding": "{tokens.spacing.xs}"
    }
  },
  "pageHeader": {
    "md": {
      "fontStyle": "title-4"
    },
    "lg": {
      "fontStyle": "title-3"
    }
  },
  "message": {
    "lg": {
      "fontStyle": "label-3",
      "gap": "{tokens.spacing.lg}",
      "padding": ["{tokens.spacing.xl}", "{tokens.spacing.xxl}"],
      "borderRadius": "{tokens.roundness.infinity}"
    },
    "md": {
      "fontStyle": "label-3",
      "gap": "{tokens.spacing.lg}",
      "padding": ["{tokens.spacing.lg}", "{tokens.spacing.xxl}"],
      "borderRadius": "{tokens.roundness.infinity}"
    }
  },
  "resizer": {
    "activeAreaWidth": "20 * {config.scaleFactor}",
    "knobWidth": "76 * {config.scaleFactor}",
    "knobHeight": "6 * {config.scaleFactor}",
    "knobRoundness": "3 * {config.scaleFactor}"
  }
}
